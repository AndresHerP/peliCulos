<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label prefHeight="112.0" prefWidth="322.0" text="Busque la película a eliminar" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Baskerville Old Face" size="30.0" />
            </font>
        </Label>
        <VBox prefHeight="37.0" prefWidth="400.0" />
        <MenuButton fx:id="menuButton" contentDisplay="BOTTOM" mnemonicParsing="false" prefHeight="34.0" prefWidth="288.0" text="Película" textAlignment="CENTER">
            <font>
                <Font name="Baskerville Old Face" size="17.0" />
            </font>
        </MenuButton>
        <VBox prefHeight="23.0" prefWidth="400.0" />
        <Label text="Se eliminara la película..." textAlignment="CENTER">
            <font>
                <Font name="Baskerville Old Face" size="16.0" />
            </font>
        </Label>
        <VBox alignment="CENTER" prefHeight="35.0" prefWidth="400.0">
            <children>
                <Label fx:id="nombrePeli" text="..." textAlignment="CENTER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <Button fx:id="eliminarButton" mnemonicParsing="false" prefHeight="36.0" prefWidth="102.0" text="Eliminar" textAlignment="CENTER" textFill="RED" textOverrun="CLIP">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font>
        </Button>
    </children>
</VBox>
